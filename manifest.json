{
  "manifest_version": 2,
  "name": "Note ",
  "description": "It is a note extension",
  "version": "1.0",

  "browser_action": {
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"

}